{
  "/register": {
    "post": "registerNewUser" //BODY: user, pwd, firstname, surname
  },
  "/auth": {
    "post": "userLogin", //BODY: user, pwd
    "/moderator": { "post": "modLogin" }
  },
  "/refresh": {
    "get": "userRefreshToken",
    "/moderator": { "get": "modRefreshToken" }
  },
  "/logout": {
    "get": "get",
    "/moderator": { "get": "modRefreshToken" }
  },
  "/users": {
    "get": "searchUser",
    "/:userId": {
      "get": "getUser",
      "delete": "deleteUser",
      "/charAvailable": {
        "get": "getCharsAvailable"
      },
      "/channels": {
        "post": "followChannel"
      },
      "/conversations": {
        "get": "getAllUserConversations",
        "post": "createConversation",
        "/:convId": {
          "get": "getAllMessagesInConversation"
        }
      },
      "/posts": {
        "get": "getAllPostByUser",
        "post": "createPost"
      }
    }
  },
  "/messages": {
    "post": "createMessage",
    "/:id": {
      "delete": "deleteMessage"
    }
  },
  "/posts": {
    "/:id": {
      "delete": "deletePost"
    }
  },
  "/channels": {
    "get": "searchChannels",
    "post": "createChannel",
    "/:id": {
      "get": "getChannelById",
      "delete": "deleteChannel",
      "/messages": {
        "get": "getAllMessagesInChannel"
      }
    }
  }
}
